{
  "kind": "collectionType",
  "collectionName": "step_changes",
  "info": {
    "singularName": "step-change",
    "pluralName": "step-changes",
    "displayName": "Step Change",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "step": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::step.step",
      "inversedBy": "changes"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "add",
        "update",
        "delete"
      ],
      "default": "add",
      "required": true
    },
    "level": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "game_version": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::game-version.game-version",
      "inversedBy": "step_changes"
    },
    "name": {
      "type": "string",
      "required": true
    }
  }
}
