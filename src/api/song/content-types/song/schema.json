{
  "kind": "collectionType",
  "collectionName": "songs",
  "info": {
    "singularName": "song",
    "pluralName": "songs",
    "displayName": "Song",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "artist": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "arcade",
        "remix",
        "full-song",
        "shortcut"
      ],
      "required": true,
      "default": "arcade"
    },
    "bpm": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "steps": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::step.step",
      "mappedBy": "song"
    },
    "premium": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Original",
        "World Music"
      ],
      "default": "Original",
      "required": true
    }
  }
}
